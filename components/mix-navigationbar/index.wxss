.mix-navigationbar {
  position: fixed;
  z-index: 10;
  top: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-top: 20px;
  height: 44px;
  box-sizing: content-box;
  /* border-bottom: 1rpx solid #eee; */
  background-color: #eee;
}
.mix-navigationbar__hd, .mix-navigationbar__ft {
  flex-shrink: 0;
  width: 100px;
  /* height: 100%; */
  /* background: #ccc; */
}
.mix-navigationbar__hd {
  padding-left: 10px;
}
.mix-navigationbar__bd {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  /* height: 100%; */
  overflow: hidden;
  /* background: #ddd; */
}
.mix-navigationbar__txt {
  font-size: 18px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 100%;
}
.mix-navigationbar__loading {
  flex-shrink: 0;
  width: 18px;
  height: 18px;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.mix-navigationbar__back {
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  /* background: #bbb; */
}
.mix-navigationbar__back:before {
  content: '';
  display: block;
  height: 10px;
  width: 10px;
  border-width: 2px 2px 0 0;
  border-color: #000;
  border-style: solid;
  transform: matrix(-0.71,-0.71,0.71,-0.71,0,0);
  position: relative;
}
@keyframes loading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}